<h1>Books</h1>
<table>
  <% @books.each do |book| %>
  <tr>
    <th>Title</th>
    <th>body</th>
  </tr>
  <tr><%= book.title %></tr>
  <tr><%= book.body %></tr>
  <tr><a href="/books/:id"></a></tr>
  <tr><a href="books/:id/edit"></tr>
  
  <% end %>
</table>


<h3>New book</h3>
<%= form_with model: @book do |f| %>
<h5>title</h5>
<%= f.text_field :title %>

<h5>body</h5>
<%= f.text_area :body%>

<%= f.submit 'Create Book' %>

<% end %>
